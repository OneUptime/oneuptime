{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
  "jsx": "react-jsx",
    "paths": {
      "@/*": ["src/*"],
      "Common/*": ["../Common/*"]
    },
    "lib": ["es2019", "dom"],
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": ["App.tsx", "app.config.ts", "src"]
}
