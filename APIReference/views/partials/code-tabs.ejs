                     <% } else if(tab.icon === 'powershell'){ %>
                        <svg class="w-4 h-4" viewBox="0 0 24 24" fill="currentColor"><path d="M0 4.013l.72 14.256 9.996 2.631L24 17.717V4.013H0zm17.856 12.247H12.1v1.313h6.312l-.557-1.313zm-8.751-1.882l.558-.558L4.86 9.406l-.558.558 3.694 3.55-.372.864h.93l.551-.001z"/></svg>
