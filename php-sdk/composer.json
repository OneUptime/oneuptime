{
    "name": "fyipe/sdk",
    "description": "Fyipe SDK package send logs from your applications to your fyipe dashboard.",
    "license": "MIT",
    "version": "3.0.9",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "HackerBay"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/var-dumper": "^5.1",
        "fzaninotto/faker": "^1.9",
        "lrucache/lrucache": "^0.3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "overtrue/phplint": "^2.0",
        "phpstan/phpstan": "^0.12.31",
        "php-parallel-lint/php-console-color": "^0.3.0",
        "php-parallel-lint/php-console-highlighter": "^0.5.0"
    },
    "autoload": {
        "psr-0": {
            "Fyipe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Fyipe\\Tests": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "lint": "./vendor/bin/phplint ./ --exclude=vendor",
        "analyze": "./vendor/bin/phpstan analyse -l 5 src tests"
    }
}
