    "private": true,
    "type": "module",
    "dependencies": {
        "airtable": "^0.11.1",
        "body-parser": "^1.19.1",
        "chai": "^4.3.4",
        "chai-http": "^4.3.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "depcheck": "^1.4.3",
        "ejs": "^3.1.6",
