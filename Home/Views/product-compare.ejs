<!DOCTYPE html>
<html lang="en" id="home">

<meta http-equiv="content-type" content="text/html;charset=utf-8" />

<head>
    <title>OneUptime vs <%= productConfig.productName %> | Complete Comparison </title>
    <meta name="description"
        content="Compare OneUptime with <%= productConfig.productName %>. See how OneUptime provides a complete observability platform with monitoring, status pages, incident management, and on-call scheduling.">
        <%- include('head', {
    enableGoogleTagManager: typeof enableGoogleTagManager !== 'undefined' ? enableGoogleTagManager : false
}) -%>
    <style>
      .hover-lift {
        transition: transform 0.2s ease, box-shadow 0.2s ease;
      }
      .hover-lift:hover {
        transform: translateY(-2px);
        box-shadow: 0 8px 24px rgba(0,0,0,0.08);
      }
      .feature-card {
        transition: border-color 0.2s ease;
      }
      .feature-card:hover {
        border-color: #18181b;
      }
    </style>
</head>

<body>

  <%- include('nav') -%>
  <main id="main-content">

  <!-- Hero Section -->
  <div class="relative bg-zinc-950">
    <div class="mx-auto max-w-7xl px-6 py-20 sm:py-28 lg:px-8">
      <div class="mx-auto max-w-3xl text-center">
        <!-- Last Updated Badge -->
        <% if(productConfig.lastUpdated) { %>
        <div class="mb-6 flex justify-center">
          <span class="inline-flex items-center rounded-full bg-zinc-900 px-3 py-1 text-xs font-medium text-zinc-400 ring-1 ring-zinc-800">
            Updated <%= productConfig.lastUpdated %>
          </span>
        </div>
        <% } %>

        <h1 class="text-4xl font-semibold tracking-tight text-white sm:text-5xl">
          OneUptime <span class="text-zinc-500">vs</span> <%= productConfig.productName %>
        </h1>

        <p class="mt-6 text-lg leading-8 text-zinc-400">
          <%= productConfig.tagline %>
        </p>

        <!-- Feature Pills -->
        <div class="mt-8 flex justify-center gap-2 flex-wrap">
          <span class="inline-flex items-center rounded-full bg-zinc-900 px-3 py-1 text-xs font-medium text-zinc-300 ring-1 ring-zinc-800">
            Complete Platform
          </span>
          <span class="inline-flex items-center rounded-full bg-zinc-900 px-3 py-1 text-xs font-medium text-zinc-300 ring-1 ring-zinc-800">
            Open Source
          </span>
          <span class="inline-flex items-center rounded-full bg-zinc-900 px-3 py-1 text-xs font-medium text-zinc-300 ring-1 ring-zinc-800">
            Self-Hostable
          </span>
        </div>

        <div class="mt-10 flex items-center justify-center gap-x-4">
          <a href="/accounts/register" class="rounded-lg bg-white px-5 py-2.5 text-sm font-semibold text-zinc-900 hover:bg-zinc-100 transition-colors">
            Start Free Trial
          </a>
          <a href="/enterprise/demo" class="rounded-lg px-5 py-2.5 text-sm font-semibold text-zinc-300 ring-1 ring-zinc-700 hover:ring-zinc-600 hover:text-white transition-all">
            Request Demo
          </a>
        </div>
      </div>
    </div>
  </div>

  <!-- Key Advantages Section -->
  <div class="bg-zinc-50 py-20 sm:py-24">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto max-w-2xl text-center mb-16">
        <h2 class="text-3xl font-semibold tracking-tight text-zinc-900">
          Why teams choose OneUptime
        </h2>
        <p class="mt-4 text-base text-zinc-600">
          See how OneUptime compares to <%= productConfig.productName %>
        </p>
      </div>

      <div class="mx-auto max-w-5xl grid md:grid-cols-2 gap-6">
        <!-- Advantage 1: Unified Platform -->
        <div class="feature-card rounded-xl bg-white p-6 border border-zinc-200">
          <div class="flex items-center gap-3 mb-4">
            <div class="flex h-10 w-10 items-center justify-center rounded-lg bg-zinc-100">
              <svg class="h-5 w-5 text-zinc-700" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 6A2.25 2.25 0 016 3.75h2.25A2.25 2.25 0 0110.5 6v2.25a2.25 2.25 0 01-2.25 2.25H6a2.25 2.25 0 01-2.25-2.25V6zM3.75 15.75A2.25 2.25 0 016 13.5h2.25a2.25 2.25 0 012.25 2.25V18a2.25 2.25 0 01-2.25 2.25H6A2.25 2.25 0 013.75 18v-2.25zM13.5 6a2.25 2.25 0 012.25-2.25H18A2.25 2.25 0 0120.25 6v2.25A2.25 2.25 0 0118 10.5h-2.25a2.25 2.25 0 01-2.25-2.25V6zM13.5 15.75a2.25 2.25 0 012.25-2.25H18a2.25 2.25 0 012.25 2.25V18A2.25 2.25 0 0118 20.25h-2.25A2.25 2.25 0 0113.5 18v-2.25z" />
              </svg>
            </div>
            <h3 class="text-base font-semibold text-zinc-900">Unified Platform</h3>
          </div>
          <p class="text-sm text-zinc-600 leading-relaxed">
            <%= productConfig.productName %> requires 3-4 separate tools. OneUptime includes monitoring, status pages, on-call, and incidents in one platform.
          </p>
        </div>

        <!-- Advantage 2: Unlimited Team Members -->
        <div class="feature-card rounded-xl bg-white p-6 border border-zinc-200">
          <div class="flex items-center gap-3 mb-4">
            <div class="flex h-10 w-10 items-center justify-center rounded-lg bg-zinc-100">
              <svg class="h-5 w-5 text-zinc-700" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" d="M15 19.128a9.38 9.38 0 002.625.372 9.337 9.337 0 004.121-.952 4.125 4.125 0 00-7.533-2.493M15 19.128v-.003c0-1.113-.285-2.16-.786-3.07M15 19.128v.106A12.318 12.318 0 018.624 21c-2.331 0-4.512-.645-6.374-1.766l-.001-.109a6.375 6.375 0 0111.964-3.07M12 6.375a3.375 3.375 0 11-6.75 0 3.375 3.375 0 016.75 0zm8.25 2.25a2.625 2.625 0 11-5.25 0 2.625 2.625 0 015.25 0z" />
              </svg>
            </div>
            <h3 class="text-base font-semibold text-zinc-900">Unlimited Team Members</h3>
          </div>
          <p class="text-sm text-zinc-600 leading-relaxed">
            Add your entire team without restrictions. <%= productConfig.productName %> limits users or charges per-seat. OneUptime grows with your team.
          </p>
        </div>

        <!-- Advantage 3: Open Source -->
        <div class="feature-card rounded-xl bg-white p-6 border border-zinc-200">
          <div class="flex items-center gap-3 mb-4">
            <div class="flex h-10 w-10 items-center justify-center rounded-lg bg-zinc-100">
              <svg class="h-5 w-5 text-zinc-700" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" d="M17.25 6.75L22.5 12l-5.25 5.25m-10.5 0L1.5 12l5.25-5.25m7.5-3l-4.5 16.5" />
              </svg>
            </div>
            <h3 class="text-base font-semibold text-zinc-900">Open Source & Self-Hostable</h3>
          </div>
          <p class="text-sm text-zinc-600 leading-relaxed">
            OneUptime is Apache 2.0 licensed. Self-host on your infrastructure with full control. <%= productConfig.productName %> is proprietary SaaS only.
          </p>
        </div>

        <!-- Advantage 4: Unlimited Subscribers -->
        <div class="feature-card rounded-xl bg-white p-6 border border-zinc-200">
          <div class="flex items-center gap-3 mb-4">
            <div class="flex h-10 w-10 items-center justify-center rounded-lg bg-zinc-100">
              <svg class="h-5 w-5 text-zinc-700" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" d="M18 18.72a9.094 9.094 0 003.741-.479 3 3 0 00-4.682-2.72m.94 3.198l.001.031c0 .225-.012.447-.037.666A11.944 11.944 0 0112 21c-2.17 0-4.207-.576-5.963-1.584A6.062 6.062 0 016 18.719m12 0a5.971 5.971 0 00-.941-3.197m0 0A5.995 5.995 0 0012 12.75a5.995 5.995 0 00-5.058 2.772m0 0a3 3 0 00-4.681 2.72 8.986 8.986 0 003.74.477m.94-3.197a5.971 5.971 0 00-.94 3.197M15 6.75a3 3 0 11-6 0 3 3 0 016 0zm6 3a2.25 2.25 0 11-4.5 0 2.25 2.25 0 014.5 0zm-13.5 0a2.25 2.25 0 11-4.5 0 2.25 2.25 0 014.5 0z" />
              </svg>
            </div>
            <h3 class="text-base font-semibold text-zinc-900">Unlimited Status Page Subscribers</h3>
          </div>
          <p class="text-sm text-zinc-600 leading-relaxed">
            Notify all your customers during incidents. Other platforms cap subscribers or limit notifications. OneUptime has no subscriber limits.
          </p>
        </div>
      </div>
    </div>
  </div>

  <!-- Hidden Costs Warning Section -->
  <% if(productConfig.hiddenCosts && productConfig.hiddenCosts.length > 0) { %>
  <div class="bg-zinc-100 py-16 sm:py-20">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto max-w-4xl">
        <div class="text-center mb-12">
          <span class="inline-flex items-center rounded-full bg-zinc-200 px-4 py-1.5 text-sm font-medium text-zinc-700 ring-1 ring-inset ring-zinc-300 mb-4">
            <svg class="mr-2 h-4 w-4" fill="currentColor" viewBox="0 0 20 20">
              <path fill-rule="evenodd" d="M8.485 2.495c.673-1.167 2.357-1.167 3.03 0l6.28 10.875c.673 1.167-.17 2.625-1.516 2.625H3.72c-1.347 0-2.189-1.458-1.515-2.625L8.485 2.495zM10 5a.75.75 0 01.75.75v3.5a.75.75 0 01-1.5 0v-3.5A.75.75 0 0110 5zm0 9a1 1 0 100-2 1 1 0 000 2z" clip-rule="evenodd" />
            </svg>
            Hidden Costs Alert
          </span>
          <h2 class="text-3xl font-semibold tracking-tight text-zinc-900 sm:text-4xl">
            The hidden costs of <%= productConfig.productName %>
          </h2>
          <p class="mt-4 text-base text-zinc-600">
            The advertised price is just the beginning. Here's what you might not see coming.
          </p>
        </div>

        <div class="rounded-2xl bg-white shadow-sm ring-1 ring-zinc-200 overflow-hidden">
          <div class="bg-zinc-900 px-6 py-4">
            <div class="flex items-center gap-3">
              <svg class="h-6 w-6 text-zinc-400" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" d="M12 6v12m-3-2.818l.879.659c1.171.879 3.07.879 4.242 0 1.172-.879 1.172-2.303 0-3.182C13.536 12.219 12.768 12 12 12c-.725 0-1.45-.22-2.003-.659-1.106-.879-1.106-2.303 0-3.182s2.9-.879 4.006 0l.415.33M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
              </svg>
              <h3 class="text-lg font-semibold text-white">Additional costs you should know about</h3>
            </div>
          </div>
          <div class="divide-y divide-zinc-100">
            <% for(var hc=0; hc < productConfig.hiddenCosts.length; hc++) { %>
            <div class="flex items-start gap-4 px-6 py-4 hover:bg-zinc-50 transition-colors">
              <div class="flex-shrink-0 mt-1">
                <svg class="h-5 w-5 text-zinc-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zM8.28 7.22a.75.75 0 00-1.06 1.06L8.94 10l-1.72 1.72a.75.75 0 101.06 1.06L10 11.06l1.72 1.72a.75.75 0 101.06-1.06L11.06 10l1.72-1.72a.75.75 0 00-1.06-1.06L10 8.94 8.28 7.22z" clip-rule="evenodd" />
                </svg>
              </div>
              <p class="text-zinc-700"><%= productConfig.hiddenCosts[hc] %></p>
            </div>
            <% } %>
          </div>
          <div class="bg-zinc-50 px-6 py-4 border-t border-zinc-100">
            <div class="flex items-center gap-3">
              <svg class="h-6 w-6 text-zinc-700" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.857-9.809a.75.75 0 00-1.214-.882l-3.483 4.79-1.88-1.88a.75.75 0 10-1.06 1.061l2.5 2.5a.75.75 0 001.137-.089l4-5.5z" clip-rule="evenodd" />
              </svg>
              <p class="text-zinc-900 font-medium">
                OneUptime: Transparent pricing with no hidden costs. All features included.
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% } %>

  <!-- Competitor Pricing Tiers -->
  <% if(productConfig.competitorPricingTiers && productConfig.competitorPricingTiers.length > 0) { %>
  <div class="bg-white py-20 sm:py-24">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto max-w-2xl text-center mb-16">
        <h2 class="text-sm font-semibold leading-7 text-zinc-500 uppercase tracking-wide"><%= productConfig.productName %> pricing breakdown</h2>
        <p class="mt-2 text-3xl font-semibold tracking-tight text-zinc-900 sm:text-4xl">
          Understanding <%= productConfig.productName %>'s pricing tiers
        </p>
        <p class="mt-4 text-base text-zinc-600">
          See exactly what you get (and don't get) at each pricing level.
        </p>
      </div>

      <div class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-<%= Math.min(productConfig.competitorPricingTiers.length, 4) %>">
        <% for(var pt=0; pt < productConfig.competitorPricingTiers.length; pt++) { %>
        <div class="relative rounded-xl border border-zinc-200 bg-white p-6 hover:border-zinc-300 transition-colors hover-lift">
          <div class="mb-4">
            <h3 class="text-base font-semibold text-zinc-900"><%= productConfig.competitorPricingTiers[pt].name %></h3>
            <div class="mt-2 flex items-baseline gap-1">
              <span class="text-2xl font-bold text-zinc-900"><%= productConfig.competitorPricingTiers[pt].price %></span>
              <span class="text-sm text-zinc-500"><%= productConfig.competitorPricingTiers[pt].period %></span>
            </div>
          </div>

          <div class="space-y-4">
            <div>
              <p class="text-xs font-semibold text-zinc-500 uppercase tracking-wide mb-2">Includes</p>
              <ul class="space-y-2">
                <% for(var f=0; f < productConfig.competitorPricingTiers[pt].features.length; f++) { %>
                <li class="flex items-start gap-2 text-sm text-zinc-600">
                  <svg class="h-4 w-4 mt-0.5 text-zinc-400 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M16.704 4.153a.75.75 0 01.143 1.052l-8 10.5a.75.75 0 01-1.127.075l-4.5-4.5a.75.75 0 011.06-1.06l3.894 3.893 7.48-9.817a.75.75 0 011.05-.143z" clip-rule="evenodd" />
                  </svg>
                  <span><%= productConfig.competitorPricingTiers[pt].features[f] %></span>
                </li>
                <% } %>
              </ul>
            </div>

            <div class="pt-4 border-t border-zinc-100">
              <p class="text-xs font-semibold text-zinc-500 uppercase tracking-wide mb-2">Limitations</p>
              <ul class="space-y-2">
                <% for(var l=0; l < productConfig.competitorPricingTiers[pt].limitations.length; l++) { %>
                <li class="flex items-start gap-2 text-sm text-zinc-500">
                  <svg class="h-4 w-4 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zM8.28 7.22a.75.75 0 00-1.06 1.06L8.94 10l-1.72 1.72a.75.75 0 101.06 1.06L10 11.06l1.72 1.72a.75.75 0 101.06-1.06L11.06 10l1.72-1.72a.75.75 0 00-1.06-1.06L10 8.94 8.28 7.22z" clip-rule="evenodd" />
                  </svg>
                  <span><%= productConfig.competitorPricingTiers[pt].limitations[l] %></span>
                </li>
                <% } %>
              </ul>
            </div>
          </div>
        </div>
        <% } %>
      </div>

      <!-- OneUptime Alternative -->
      <div class="mt-12">
        <div class="rounded-2xl border border-zinc-900 bg-zinc-950 p-8">
          <div class="flex flex-col lg:flex-row lg:items-center lg:justify-between gap-6">
            <div>
              <div class="flex items-center gap-3 mb-3">
                <img src="/img/3-transparent.svg" alt="OneUptime" class="h-10 w-10">
                <h3 class="text-xl font-semibold text-white">OneUptime Alternative</h3>
              </div>
              <p class="text-zinc-400 max-w-2xl">
                Skip the complexity. Get monitoring, status pages, on-call scheduling, and incident management - all included from day one with no subscriber limits or per-user fees.
              </p>
            </div>
            <div class="flex flex-col sm:flex-row gap-4">
              <a href="/accounts/register" class="inline-flex items-center justify-center rounded-lg bg-white px-6 py-3 text-base font-semibold text-zinc-900 hover:bg-zinc-100 transition-all whitespace-nowrap">
                Start Free
              </a>
              <a href="/pricing" class="inline-flex items-center justify-center rounded-lg border border-zinc-700 px-6 py-3 text-base font-semibold text-zinc-300 hover:border-zinc-600 hover:text-white transition-all whitespace-nowrap">
                View Pricing
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% } %>

  <!-- Real-World Use Cases Comparison -->
  <% if(productConfig.useCases && productConfig.useCases.length > 0) { %>
  <div class="bg-zinc-950 py-20 sm:py-24">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto max-w-2xl text-center mb-16">
        <h2 class="text-sm font-semibold leading-7 text-zinc-400 uppercase tracking-wide">Real-world scenarios</h2>
        <p class="mt-2 text-3xl font-semibold tracking-tight text-white sm:text-4xl">
          See the actual savings
        </p>
        <p class="mt-4 text-base text-zinc-400">
          Compare total cost of ownership for common team setups.
        </p>
      </div>

      <div class="space-y-6">
        <% for(var uc=0; uc < productConfig.useCases.length; uc++) { %>
        <div class="rounded-xl bg-zinc-900 ring-1 ring-zinc-800 overflow-hidden hover-lift">
          <div class="bg-zinc-900 px-6 py-4 border-b border-zinc-800">
            <h3 class="text-base font-semibold text-white flex items-center gap-3">
              <span class="flex h-8 w-8 items-center justify-center rounded-lg bg-zinc-800 text-sm font-bold text-zinc-400"><%= uc + 1 %></span>
              <%= productConfig.useCases[uc].scenario %>
            </h3>
          </div>
          <div class="grid lg:grid-cols-2 divide-y lg:divide-y-0 lg:divide-x divide-zinc-800">
            <!-- Competitor Solution -->
            <div class="p-6">
              <div class="flex items-center gap-2 mb-4">
                <div class="flex h-8 w-8 items-center justify-center rounded-lg bg-zinc-800">
                  <span class="text-sm font-bold text-zinc-500"><%= productConfig.productName.charAt(0) %></span>
                </div>
                <span class="text-sm font-medium text-zinc-500">With <%= productConfig.productName %></span>
              </div>
              <p class="text-zinc-400 text-sm mb-4"><%= productConfig.useCases[uc].competitorSolution %></p>
              <div class="flex items-baseline gap-2">
                <span class="text-2xl font-bold text-zinc-300"><%= productConfig.useCases[uc].competitorCost %></span>
              </div>
            </div>
            <!-- OneUptime Solution -->
            <div class="p-6 bg-white/5">
              <div class="flex items-center gap-2 mb-4">
                <img src="/img/3-transparent.svg" alt="OneUptime" class="h-8 w-8">
                <span class="text-sm font-medium text-white">With OneUptime</span>
              </div>
              <p class="text-zinc-400 text-sm mb-4"><%= productConfig.useCases[uc].oneuptimeSolution %></p>
              <div class="flex items-baseline gap-2">
                <span class="text-2xl font-bold text-white"><%= productConfig.useCases[uc].oneuptimeCost %></span>
              </div>
            </div>
          </div>
        </div>
        <% } %>
      </div>
    </div>
  </div>
  <% } %>

  <!-- Key Differences Section -->
  <% if(productConfig.keyDifferences && productConfig.keyDifferences.length > 0) { %>
  <div class="bg-zinc-100 py-20 sm:py-24">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <div class="mx-auto max-w-2xl text-center mb-16">
        <h2 class="text-sm font-semibold leading-7 text-zinc-500 uppercase tracking-wide">Why choose OneUptime</h2>
        <p class="mt-2 text-3xl font-semibold tracking-tight text-zinc-900 sm:text-4xl">
          Key advantages over <%= productConfig.productName %>
        </p>
      </div>

      <div class="mx-auto max-w-5xl">
        <div class="grid grid-cols-1 gap-6 sm:grid-cols-2">
          <% for(var k=0; k < productConfig.keyDifferences.length; k++) { %>
          <div class="relative rounded-xl bg-white p-6 border border-zinc-200 hover:border-zinc-300 transition-colors">
            <div class="flex items-start gap-4">
              <div class="flex h-10 w-10 flex-shrink-0 items-center justify-center rounded-lg bg-zinc-100">
                <% if(productConfig.keyDifferences[k].icon === "monitoring") { %>
                <svg class="h-5 w-5 text-zinc-700" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 3v11.25A2.25 2.25 0 006 16.5h2.25M3.75 3h-1.5m1.5 0h16.5m0 0h1.5m-1.5 0v11.25A2.25 2.25 0 0118 16.5h-2.25m-7.5 0h7.5m-7.5 0l-1 3m8.5-3l1 3m0 0l.5 1.5m-.5-1.5h-9.5m0 0l-.5 1.5m.75-9l3-3 2.148 2.148A12.061 12.061 0 0116.5 7.605" />
                </svg>
                <% } else if(productConfig.keyDifferences[k].icon === "status-page") { %>
                <svg class="h-5 w-5 text-zinc-700" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" d="M9 12.75L11.25 15 15 9.75M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
                </svg>
