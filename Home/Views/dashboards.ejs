            </div>
            <div class="text-2xl font-bold text-white">Shareable</div>
            <div class="mt-1 text-sm text-indigo-100">Links</div>
          </div>
        </div>
      </div>
    </div>

    <%- include('logo-roll') -%>

    <!-- How It Works -->
    <div class="relative bg-gradient-to-b from-white via-indigo-50/30 to-white py-24 sm:py-32 overflow-hidden">
