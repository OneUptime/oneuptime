<span class="text-purple-400">let</span> span = tracer.<span class="text-cyan-400">spanBuilder</span>(spanName: <span class="syntax-string">"operation"</span>).<span class="text-cyan-400">startSpan</span>()
span.<span class="text-cyan-400">end</span>()`
      }
    };

    let currentLang = null;

    // Color mappings for each language
    const langColors = {
      go: { bg: 'bg-cyan-500/20', border: 'border-cyan-500/50', text: 'text-cyan-400', shadow: 'shadow-cyan-500/30' },
      java: { bg: 'bg-red-500/20', border: 'border-red-500/50', text: 'text-red-400', shadow: 'shadow-red-500/30' },
      python: { bg: 'bg-yellow-500/20', border: 'border-yellow-500/50', text: 'text-yellow-400', shadow: 'shadow-yellow-500/30' },
      nodejs: { bg: 'bg-green-500/20', border: 'border-green-500/50', text: 'text-green-400', shadow: 'shadow-green-500/30' },
      dotnet: { bg: 'bg-purple-500/20', border: 'border-purple-500/50', text: 'text-purple-400', shadow: 'shadow-purple-500/30' },
      ruby: { bg: 'bg-rose-500/20', border: 'border-rose-500/50', text: 'text-rose-400', shadow: 'shadow-rose-500/30' },
      php: { bg: 'bg-indigo-500/20', border: 'border-indigo-500/50', text: 'text-indigo-400', shadow: 'shadow-indigo-500/30' },
      rust: { bg: 'bg-orange-500/20', border: 'border-orange-500/50', text: 'text-orange-400', shadow: 'shadow-orange-500/30' },
      cpp: { bg: 'bg-blue-500/20', border: 'border-blue-500/50', text: 'text-blue-400', shadow: 'shadow-blue-500/30' },
      swift: { bg: 'bg-orange-500/20', border: 'border-orange-500/50', text: 'text-orange-400', shadow: 'shadow-orange-500/30' }
    };

    window.switchLang = function(lang) {
      if (!codeSnippets[lang]) return;

      currentLang = lang;
      const codeContent = document.getElementById('code-content');
      const terminalFilename = document.getElementById('terminal-filename');

      if (codeContent) {
        codeContent.innerHTML = '<code>' + codeSnippets[lang].code + '</code>';
      }
      if (terminalFilename) {
        terminalFilename.textContent = codeSnippets[lang].filename;
      }

      // Update active state for language buttons
      const langBtns = document.querySelectorAll('.lang-btn');
      langBtns.forEach(function(btn) {
        const btnLang = btn.getAttribute('data-lang');
        const icon = btn.querySelector('.lang-icon');
        const label = btn.querySelector('.lang-label');

        if (btnLang === lang) {
          const colors = langColors[lang];
          icon.classList.add(colors.bg, colors.border, 'scale-110', 'shadow-lg', colors.shadow);
          icon.classList.remove('bg-slate-800/80', 'border-slate-700/50');
          icon.querySelector('svg').classList.add(colors.text);
          icon.querySelector('svg').classList.remove('text-slate-400');
          label.classList.add(colors.text);
          label.classList.remove('text-slate-500');
        } else {
          // Reset to default state
          const prevColors = langColors[btnLang];
          icon.classList.remove(prevColors.bg, prevColors.border, 'scale-110', 'shadow-lg', prevColors.shadow);
          icon.classList.add('bg-slate-800/80', 'border-slate-700/50');
          icon.querySelector('svg').classList.remove(prevColors.text);
          icon.querySelector('svg').classList.add('text-slate-400');
          label.classList.remove(prevColors.text);
          label.classList.add('text-slate-500');
        }
      });
    };

    window.copyCode = function() {
      const codeContent = document.getElementById('code-content');
      if (!codeContent) return;

      // Get plain text from HTML content
      const tempDiv = document.createElement('div');
      tempDiv.innerHTML = codeContent.innerHTML;
      const plainText = tempDiv.textContent || tempDiv.innerText;

      navigator.clipboard.writeText(plainText).then(function() {
        // Show copied feedback
        const copyBtn = document.querySelector('.copy-btn');
        if (copyBtn) {
          const originalText = copyBtn.innerHTML;
          copyBtn.innerHTML = '<svg class="w-3.5 h-3.5" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" d="m4.5 12.75 6 6 9-13.5" /></svg> Copied!';
          setTimeout(function() {
            copyBtn.innerHTML = originalText;
          }, 2000);
        }
      });
    };

    // No language selected by default - shows OTel Collector config
  })();
  </script>

</body>

