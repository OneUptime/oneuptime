    }
    .dev-glow-cyan:hover {
        box-shadow: 0 0 0 1px rgb(6 182 212 / 0.2), 0 4px 16px -4px rgb(6 182 212 / 0.15), 0 0 24px -8px rgb(6 182 212 / 0.1);
    }

    /* Hero card wrappers */
    .hero-card-wrapper {
        position: relative;
    }

    /* Code block styling */
    .code-block {
        font-family: 'SF Mono', 'Menlo', 'Monaco', 'Consolas', monospace;
        font-size: 13px;
        line-height: 1.6;
    }
</style>
</head>

