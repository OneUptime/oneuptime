                    <img src="/img/incidents.png" alt="OneUptime incident management dashboard showing active incidents and timelines" width="2432" height="1442"
                        class="w-full rounded-lg ring-1 ring-gray-200">
                    <%- include('./Partials/watch-demo.ejs', {videoId: "_fQ_F4EisBQ"}) -%>
                </div>
            </div>
        </div>
    </div>

    <!-- Stats Section -->
    <div class="border-y border-gray-200 bg-gray-50 py-16">
        <div class="mx-auto max-w-7xl px-6 lg:px-8">
            <div class="grid grid-cols-2 gap-8 md:grid-cols-4">
                <div class="text-center">
                    <div class="text-3xl font-bold text-gray-900">70%</div>
                    <div class="mt-2 text-sm text-gray-600">Faster MTTR</div>
                </div>
                <div class="text-center">
                    <div class="text-3xl font-bold text-gray-900">Real-time</div>
                    <div class="mt-2 text-sm text-gray-600">Collaboration</div>
                </div>
                <div class="text-center">
                    <div class="text-3xl font-bold text-gray-900">100%</div>
                    <div class="mt-2 text-sm text-gray-600">Audit Trail</div>
                </div>
                <div class="text-center">
                    <div class="text-3xl font-bold text-gray-900">5000+</div>
                    <div class="mt-2 text-sm text-gray-600">Integrations</div>
                </div>
            </div>
        </div>
    </div>

    <%- include('logo-roll') -%>

    <!-- How It Works Section -->
    <div class="bg-white py-24 sm:py-32">
        <div class="mx-auto max-w-7xl px-6 lg:px-8">
            <div class="mx-auto max-w-2xl text-center mb-16">
                <p class="text-base font-semibold text-indigo-600">How It Works</p>
                <h2 class="mt-2 text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">From alert to resolution in three steps</h2>
            </div>

            <div class="grid grid-cols-1 md:grid-cols-3 gap-8 lg:gap-12">
                <!-- Step 1 -->
                <div class="flex flex-col items-center text-center">
                    <div class="flex h-14 w-14 items-center justify-center rounded-full bg-indigo-600 text-white text-xl font-bold mb-6">1</div>
                    <h3 class="text-lg font-semibold text-gray-900 mb-3">Detect & Alert</h3>
                    <p class="text-sm text-gray-600">Monitors detect issues and automatically create incidents. Alert the right team members instantly via their preferred channels.</p>
                </div>

                <!-- Step 2 -->
                <div class="flex flex-col items-center text-center">
                    <div class="flex h-14 w-14 items-center justify-center rounded-full bg-indigo-600 text-white text-xl font-bold mb-6">2</div>
                    <h3 class="text-lg font-semibold text-gray-900 mb-3">Investigate & Collaborate</h3>
                    <p class="text-sm text-gray-600">Team members collaborate in real-time. Update incident status, add notes, and track the timeline as you work towards resolution.</p>
                </div>

                <!-- Step 3 -->
                <div class="flex flex-col items-center text-center">
                    <div class="flex h-14 w-14 items-center justify-center rounded-full bg-indigo-600 text-white text-xl font-bold mb-6">3</div>
                    <h3 class="text-lg font-semibold text-gray-900 mb-3">Resolve & Learn</h3>
                    <p class="text-sm text-gray-600">Resolve incidents, update your status page automatically, and generate postmortem reports to prevent future occurrences.</p>
                </div>
            </div>
        </div>
    </div>

    <!-- Core Features Grid -->
    <div class="bg-gray-50 py-24 sm:py-32">
        <div class="mx-auto max-w-7xl px-6 lg:px-8">
            <div class="mx-auto max-w-2xl text-center">
                <p class="text-base font-semibold text-indigo-600">Complete Incident Lifecycle</p>
                <h2 class="mt-2 text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Everything you need to manage incidents</h2>
                <p class="mt-6 text-lg leading-8 text-gray-600">From automatic detection to post-incident analysis, OneUptime provides a complete incident management solution.</p>
            </div>
            <div class="mx-auto mt-16 max-w-7xl">
                <div class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-3">
                    <!-- Feature 1: Instant Detection -->
                    <div class="rounded-lg bg-white p-6 ring-1 ring-gray-200">
