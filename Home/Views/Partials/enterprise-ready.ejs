                    <h3 class="text-lg font-semibold text-gray-900 mb-2">Custom Contracts</h3>
                    <p class="text-sm text-gray-500 leading-relaxed">Flexible terms tailored to your organization's needs.</p>
                </div>
            </div>

        </div>

        <!-- Bottom CTA -->
        <div class="mt-14 text-center">
            <a href="/enterprise/overview" class="inline-flex items-center gap-3 px-6 py-3 text-sm font-medium text-white bg-gray-900 rounded-full hover:bg-gray-900 transition-colors group">
                Explore enterprise features
                <svg class="w-4 h-4 group-hover:trangray-x-0.5 transition-transform" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M13.5 4.5 21 12m0 0-7.5 7.5M21 12H3" />
                </svg>
            </a>
        </div>
    </div>
</section>

<script>
(function() {
    const ambientGlow = document.getElementById('enterprise-ambient-glow');
    const cardWrappers = document.querySelectorAll('.enterprise-card-wrapper');
    let currentColor = null;
    let fadeTimeout = null;

    const glowColors = {
        'glow-emerald-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(16 185 129 / 0.10) 0%, rgb(16 185 129 / 0.04) 30%, rgb(16 185 129 / 0.02) 50%, transparent 70%)',
        'glow-violet-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(139 92 246 / 0.10) 0%, rgb(139 92 246 / 0.04) 30%, rgb(139 92 246 / 0.02) 50%, transparent 70%)',
        'glow-blue-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(59 130 246 / 0.10) 0%, rgb(59 130 246 / 0.04) 30%, rgb(59 130 246 / 0.02) 50%, transparent 70%)',
        'glow-gray-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(99 102 241 / 0.10) 0%, rgb(99 102 241 / 0.04) 30%, rgb(99 102 241 / 0.02) 50%, transparent 70%)',
        'glow-cyan-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(6 182 212 / 0.10) 0%, rgb(6 182 212 / 0.04) 30%, rgb(6 182 212 / 0.02) 50%, transparent 70%)',
        'glow-amber-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(245 158 11 / 0.10) 0%, rgb(245 158 11 / 0.04) 30%, rgb(245 158 11 / 0.02) 50%, transparent 70%)',
        'glow-rose-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(244 63 94 / 0.10) 0%, rgb(244 63 94 / 0.04) 30%, rgb(244 63 94 / 0.02) 50%, transparent 70%)',
        'glow-yellow-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(234 179 8 / 0.10) 0%, rgb(234 179 8 / 0.04) 30%, rgb(234 179 8 / 0.02) 50%, transparent 70%)',
        'glow-teal-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(20 184 166 / 0.10) 0%, rgb(20 184 166 / 0.04) 30%, rgb(20 184 166 / 0.02) 50%, transparent 70%)',
        'glow-gray-wrapper': 'radial-gradient(ellipse at 70% 70%, rgb(100 116 139 / 0.10) 0%, rgb(100 116 139 / 0.04) 30%, rgb(100 116 139 / 0.02) 50%, transparent 70%)'
    };

    function getGlowColor(wrapper) {
        const wrapperClasses = wrapper.className.split(' ');
        for (var i = 0; i < wrapperClasses.length; i++) {
            if (glowColors[wrapperClasses[i]]) {
                return glowColors[wrapperClasses[i]];
            }
        }
        return null;
    }

    cardWrappers.forEach(function(wrapper) {
        wrapper.addEventListener('mouseenter', function() {
            const newColor = getGlowColor(wrapper);

            if (fadeTimeout) {
                clearTimeout(fadeTimeout);
                fadeTimeout = null;
            }

            if (currentColor && currentColor !== newColor) {
                // Fade out first, then fade in new color
                ambientGlow.style.opacity = '0';
                fadeTimeout = setTimeout(function() {
                    ambientGlow.style.background = newColor;
                    ambientGlow.style.opacity = '1';
                    currentColor = newColor;
                }, 250);
            } else if (!currentColor) {
                // No current color, just fade in
                ambientGlow.style.background = newColor;
                ambientGlow.style.opacity = '1';
                currentColor = newColor;
            }
        });

        wrapper.addEventListener('mouseleave', function() {
            if (fadeTimeout) {
                clearTimeout(fadeTimeout);
                fadeTimeout = null;
            }

            // Small delay before fading out to allow for card-to-card transitions
            fadeTimeout = setTimeout(function() {
                ambientGlow.style.opacity = '0';
                currentColor = null;
            }, 100);
        });
    });
})();
</script>
