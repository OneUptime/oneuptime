<style>
  .payg-tab-active {
    border-color: #111827 !important;
    color: #111827 !important;
  }
  .payg-tab:focus {
    outline: none;
  }

  /* Bitcoin Easter Egg Styles - Monochrome */
  .bitcoin-easter-egg {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #000;
    z-index: 9999;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    cursor: pointer;
    font-family: 'Courier New', 'Monaco', 'Menlo', monospace;
    padding: 2rem;
  }

  .bitcoin-easter-egg.active {
    display: flex;
  }

  .bitcoin-ascii {
    color: #fff;
    font-size: 6px;
    line-height: 1.1;
    white-space: pre;
    letter-spacing: 1px;
  }

  @media (min-width: 480px) {
    .bitcoin-ascii {
      font-size: 8px;
    }
  }

  @media (min-width: 640px) {
    .bitcoin-ascii {
      font-size: 12px;
    }
  }

  @media (min-width: 900px) {
    .bitcoin-ascii {
      font-size: 16px;
    }
  }

  .genesis-text {
    color: #fff;
    font-size: 0.875rem;
    margin-top: 3rem;
    text-align: left;
    max-width: 600px;
    line-height: 1.6;
    font-family: 'Courier New', 'Monaco', 'Menlo', monospace;
  }

  .genesis-text::before {
    content: '> ';
    opacity: 0.5;
  }

  @media (min-width: 640px) {
    .genesis-text {
      font-size: 1.1rem;
    }
  }

  .bitcoin-hint {
    color: #fff;
    opacity: 0.3;
    font-size: 0.75rem;
    position: absolute;
    bottom: 2rem;
    letter-spacing: 1px;
    font-family: 'Courier New', 'Monaco', 'Menlo', monospace;
  }
</style>

