          <script>
            // Bitcoin Easter Egg - Type "bitcoin" to activate
            (function() {
              const secretWord = 'bitcoin';
              let typedKeys = '';

              function showBitcoinEasterEgg() {
                const easterEgg = document.getElementById('bitcoin-easter-egg');
                easterEgg.classList.add('active');
              }

              window.closeBitcoinEasterEgg = function() {
                const easterEgg = document.getElementById('bitcoin-easter-egg');
                easterEgg.classList.remove('active');
                typedKeys = '';
              };

              document.addEventListener('keydown', function(e) {
                const easterEgg = document.getElementById('bitcoin-easter-egg');

                // If easter egg is active, close it on any key press
                if (easterEgg.classList.contains('active')) {
                  closeBitcoinEasterEgg();
                  return;
                }

                // Ignore if user is typing in an input/textarea
                if (e.target.tagName === 'INPUT' || e.target.tagName === 'TEXTAREA') return;

                typedKeys += e.key.toLowerCase();

                // Keep only the last N characters where N is the length of secret word
                if (typedKeys.length > secretWord.length) {
                  typedKeys = typedKeys.slice(-secretWord.length);
                }

                // Check if the secret word was typed
                if (typedKeys === secretWord) {
                  showBitcoinEasterEgg();
                  typedKeys = '';
                }
              });
            })();
          </script>

